{
  "type": "module",
  "name": "openupm-next",
  "version": "1.0.0",
  "description": "openupm-next in typescript",
  "private": true,
  "main": "index.js",
  "scripts": {
    "precompile": "node precompile.js",
    "precompile:dev": "npm run precompile -- --watch",
    "docs:dev": "concurrently -k -n precompile:dev,docs:dev \"npm run precompile:dev\" \"vuepress dev docs\"",
    "docs:build": "npm run precompile && NODE_OPTIONS='--experimental-modules --es-module-specifier-resolution=node --max-old-space-size=14000' vuepress build docs",
    "docs:build:limit": "npm run precompile && PACKAGE_LIMIT=5 NODE_OPTIONS='--experimental-modules --es-module-specifier-resolution=node --max-old-space-size=14000' vuepress build docs",
    "docs:serve": "ws -d ./docs/.vuepress/dist --cors.origin '*'",
    "test:dev": "cross-env NODE_ENV=test vitest --config vite.config.test.ts",
    "test": "cross-env NODE_ENV=test vitest run --config vite.config.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openupm/openupm-next.git"
  },
  "keywords": [
    "openupm"
  ],
  "author": "Favo Yang",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/openupm/openupm-next/issues"
  },
  "homepage": "https://github.com/openupm/openupm-next#readme",
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@types/chai": "^4.3.5",
    "@types/escape-html": "^1.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.7",
    "@types/numeral": "^2.0.2",
    "@vuepress/client": "^2.0.0-beta.67",
    "@vuepress/plugin-register-components": "^2.0.0-beta.67",
    "@vuepress/plugin-search": "^2.0.0-beta.67",
    "@vuepress/plugin-theme-data": "^2.0.0-beta.67",
    "@vueuse/core": "^10.2.1",
    "axios": "^1.4.0",
    "chai": "^4.3.7",
    "change-case": "^4.1.2",
    "concurrently": "^8.2.0",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^2.30.0",
    "esbuild": "^0.18.8",
    "escape-html": "^1.0.3",
    "highlight.js": "^11.8.0",
    "joi": "^17.9.2",
    "js-yaml": "^4.1.0",
    "local-web-server": "^5.3.0",
    "lodash-es": "^4.17.21",
    "numeral": "^2.0.6",
    "pinia": "^2.1.3",
    "pinia-plugin-persistedstate": "^3.1.0",
    "query-string": "^8.1.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "spectre.css": "^0.5.9",
    "url-join": "^5.0.0",
    "v-lazy-image": "^2.1.1",
    "vitest": "^0.32.2",
    "vue": "^3.3.4",
    "vue-content-loader": "^2.0.1",
    "vue-i18n": "^9.2.2",
    "vue-scrollto": "^2.20.0",
    "vue-virtual-scroll-grid": "^1.10.2",
    "vue3-mq": "^3.1.3",
    "vuepress": "^2.0.0-beta.67",
    "vuepress-plugin-seo2": "^2.0.0-beta.238",
    "vuepress-plugin-sitemap2": "^2.0.0-beta.238"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "spdx-license-list": "^6.6.0"
  },
  "volta": {
    "node": "18.18.2"
  }
}