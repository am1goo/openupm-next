{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "ES6",
        "module": "ESNext",
        "moduleResolution": "node",
        "lib": ["ESNext", "DOM", "DOM.Iterable"],
        "paths": {
            "@/*": [
                "./docs/.vuepress/*"
            ]
        }
    },
    "include": [
        "env.d.ts",
        "docs/.vuepress/*.ts",
        "docs/.vuepress/*.vue",
        "docs/.vuepress/**/*.ts",
        "docs/.vuepress/**/*.vue"
    ],
    "exclude": [
        "node_modules",
        ".temp",
        "lib",
        "dist"
    ]
}