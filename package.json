  },
  "homepage": "https://github.com/openupm/openupm-next#readme",
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@types/chai": "^4.3.5",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.7",
    "@types/numeral": "^2.0.2",
    "@vuepress/client": "^2.0.0-beta.63",
    "@vuepress/plugin-register-components": "^2.0.0-beta.63",
    "@vuepress/plugin-theme-data": "^2.0.0-beta.63",
